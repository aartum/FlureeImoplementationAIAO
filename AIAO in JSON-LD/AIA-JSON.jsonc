        "time": "http://www.w3.org/2006/time#",
        "prov": "http://www.w3.org/ns/prov#",
        "gps": "http://purl.org/aiaontology#GPS",
        "point": "http://purl.org/aiaontology#Point",
        "state": "http://purl.org/aiaontology#State",
        "statediff": "http://purl.org/aiaontology#StateDiff",
        "CRS": "http://purl.org/aiaontology#CRS",
        "StateDiff": "http://purl.org/aiaontology#StateDiff",
        "GPS": "http://purl.org/aiaontology#GPS"
    },
    "ledger": "aiao/base",
    "insert": [
        {
            "@id": "_:bnode1",
            "@type": "owl:Axiom",
            "owl:annotatedSource": {
                "@type": "owl:Restriction",
                "owl:onProperty": ":hasDuration",
                "owl:someValuesFrom": "time:Duration",
                "rdfs:subClassOf": {
                    "owl:intersectionOf": [
                        {
                            "@type": "owl:Restriction",
                            "owl:onProperty": "prov:endedAtTime",
                            "owl:someValuesFrom": "xsd:dateTime"
                        },
                        {
                            "@type": "owl:Restriction",
                            "owl:onProperty": "prov:startedAtTime",
                            "owl:someValuesFrom": "xsd:dateTime"
                        }
                    ],
                    "@type": "owl:Class"
                }
            },
            "owl:annotatedProperty": "rdfs:subClassOf",
            "owl:annotatedTarget": {
                "owl:intersectionOf": [
                    {
                        "@type": "owl:Restriction",
                        "owl:onProperty": "prov:endedAtTime",
                        "owl:someValuesFrom": "xsd:dateTime"
                    },
                    {
                        "@type": "owl:Restriction",
                        "owl:onProperty": "prov:startedAtTime",
                        "owl:someValuesFrom": "xsd:dateTime"
                    }
                ],
                "@type": "owl:Class"
            },
            "rdfs:comment": "The class of all things that have a duration is a subclass of the set of all things that have a start as well as end time. In other words, everything which has a duration must have started at some time and ended at some time."
        },
        {
            "@id": "_:bnode2",
            "@type": "owl:Axiom",
            "owl:annotatedSource": {
                "@type": "owl:Restriction",
                "owl:onProperty": ":isStateDiffFrom",
                "owl:someValuesFrom": ":State",
                "rdfs:subClassOf": {"@id":"StateDiff"}
            },
            "owl:annotatedProperty": "rdfs:subClassOf",
            "owl:annotatedTarget": ":StateDiff",
            "rdfs:comment": "Two different States of the same Environment (asserted by using isStateDiffFrom) is an StateDiff entity."
        },
        {
            "@id": "_:bnode3",
            "@type": "owl:Axiom",
            "owl:annotatedSource": {
                "@type": "owl:Restriction",
                "owl:onProperty": ":hasCRS",
                "owl:hasValue": ":GPS",
                "rdfs:subClassOf": {
                    "@type": "owl:Restriction",
                    "owl:onProperty": ":hasType",
                    "owl:someValuesFrom": ":Point"
                }
            },
            "owl:annotatedProperty": "rdfs:subClassOf",
            "owl:annotatedTarget": {
                "@type": "owl:Restriction",
                "owl:onProperty": ":hasType",
                "owl:someValuesFrom": ":Point"
            },
            "rdfs:comment": "This is just an example of a specific CRS corresponding to a specific Location Type (Point, Line, Path, Polygon, Volume). Many more may be added."
        }
    ]
