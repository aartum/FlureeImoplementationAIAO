      },
      {
        "@id": "_:bnode1",
        "@type": "owl:Axiom",
        "owl:annotatedSource": {
          "@type": "owl:Class",
          "owl:intersectionOf": [
            ":Claim",
            {
              "@type": "owl:Restriction",
              "owl:onProperty": ":regardsVeracity",
              "owl:someValuesFrom": ":Claim"
            }
          ]
        },
        "owl:annotatedProperty": "owl:equivalentClass",
        "owl:annotatedTarget": {
          "@type": "owl:Restriction",
          "owl:onProperty": ":verifies",
          "owl:someValuesFrom": ":Claim"
        },
        "rdfs:comment": "(Auditor makesClaim Claim1) ^ (Claim1 regardsVeracity Claim2) iff (Auditor verifies Claim2); i.e. if the statements on the left/right are true, those on the right/left are as well."
      }
    ]
  }


